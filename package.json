{
    "name": "observablejs",
    "version": "0.1.0",
    "description": "A JavaScript framework with built-in observability for monitoring application performance",
    "private": true,
    "workspaces": [
        "packages/*"
    ],
    "scripts": {
        "build": "npm run build --workspaces",
        "test": "npm run test --workspaces",
        "lint": "eslint packages/*/src/**/*.js",
        "start:examples": "cd packages/examples && npm start",
        "dev": "vite"
    },
    "keywords": [
        "javascript",
        "framework",
        "observability",
        "performance",
        "metrics",
        "reactive"
    ],
    "author": "",
    "license": "MIT",
    "devDependencies": {
        "eslint": "^8.53.0",
        "jest": "^29.7.0",
        "vite": "^5.4.18"
    },
    "dependencies": {
        "-": "^0.0.1"
    }
}
